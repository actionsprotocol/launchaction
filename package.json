{
  "name": "agent-twitter-client",
  "description": "A twitter client for agents",
  "keywords": [],
  "version": "0.0.18",
  "main": "dist/default/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "node": {
      "import": "./dist/node/esm/index.mjs",
      "require": "./dist/node/cjs/index.cjs"
    },
    "default": {
      "import": "./dist/default/esm/index.mjs",
      "require": "./dist/default/cjs/index.js"
    }
  },
  "author": "elizaOS",
  "license": "MIT",
  "scripts": {
    "start": "tsx src/server/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx' src/server/index.ts",
    "docs:generate": "typedoc --options typedoc.json",
    "docs:deploy": "npm run docs:generate && gh-pages -d docs",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@roamhq/wrtc": "^0.8.0",
    "@sinclair/typebox": "^0.32.20",
    "@solana/web3.js": "^1.98.0",
    "drizzle-orm": "^0.39.3",
    "fastify": "^4.26.2",
    "headers-polyfill": "^3.1.2",
    "json-stable-stringify": "^1.0.2",
    "otpauth": "^9.2.2",
    "pg": "^8.13.3",
    "set-cookie-parser": "^2.6.0",
    "tough-cookie": "^4.1.2",
    "twitter-api-v2": "^1.18.2",
    "undici": "^7.1.1",
    "undici-types": "^7.2.0",
    "ws": "^8.18.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@fastify/type-provider-typebox": "^3.0.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.11",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/tough-cookie": "^4.0.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.30.4",
    "esbuild": "^0.21.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^5.0.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^3.1.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "tsx": "^4.19.3",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.2.1+sha512.398035c7bd696d0ba0b10a688ed558285329d27ea994804a52bad9167d8e3a72bcb993f9699585d3ca25779ac64949ef422757a6c31102c12ab932e5cbe5cc92"
